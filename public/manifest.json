{
    "manifest_version": 2,
    "name": "Workspaces",
    "version": "1.0",
    "icons": {
        "16": "images/workspaces-icon-16.png",
        "48": "images/workspaces-icon-48.png",
        "128": "images/workspaces-icon-128.png"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js",
            "workspace-tab-organizer.js"
        ],
        "persistent": false
    },
    "options_ui": {
        "page": "index.html",
        "open_in_tab": true
    },
    "web_accessible_resources": [
        "build/bundle.js.map"
    ]
}
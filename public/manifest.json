{
    "manifest_version": 2,
    "name": "Workspaces",
    "version": "1.0.5",
    "default_locale": "en",
    "description": "Manage your work by grouping tabs from similar domains into a single chrome window",
    "icons": {
        "16": "images/workspaces-icon-16.png",
        "48": "images/workspaces-icon-48.png",
        "128": "images/workspaces-icon-128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "images/workspaces-icon-16.png",
            "24": "images/workspaces-icon-24.png",
            "32": "images/workspaces-icon-32.png"
        }
    },
    "permissions": ["tabs", "storage"],
    "background": {
        "scripts": ["storage-service.js", "build/background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["storage-service.js", "content.js"]
    }],
    "options_ui": {
        "page": "index.html",
        "open_in_tab": true
    },
    "web_accessible_resources": ["build/bundle.css.map"]
}
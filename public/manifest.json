{
    "manifest_version": 2,
    "name": "URL to Window",
    "version": "0.1",
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": ["background.js", "workspace-tab-organizer.js"],
        "persistent": false
    },
    "options_ui": {
        "page": "index.html",
        "open_in_tab": true
    }
}